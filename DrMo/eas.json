{
  "cli": {
    "version": ">= 3.9.3"
  },
  "build": {
    "release": {
      "channel": "production"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      } 
    },
    
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": true
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
